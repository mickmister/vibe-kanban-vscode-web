{
  "name": "vibe-kanban-vscode-web",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "node dist/node/node-entry.mjs",
    "build": "npm run build:web && npm run build:node",
    "build:web": "SPRINGBOARD_PLATFORM=web vite build",
    "build:node": "SPRINGBOARD_PLATFORM=node vite build --outDir dist/node",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "better-sqlite3": "^12.6.2",
    "crossws": "^0.4.4",
    "hono": "^4.11.9",
    "immer": "^11.1.4",
    "kysely": "^0.28.11",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router": "^7.13.0",
    "rxjs": "^7.8.2",
    "springboard": "0.0.1-dev-vite-with-docs-7"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  }
}